doctype html
html
  head
    title 게시판
    meta(charset='utf-8')
    script(src='/js/board.js')
  body

    include ../includes/header.pug

    ul
      each list in boardList
        li
          a(href='/main/boardList/'+list)= list

    article
      h2= title
      div= content

    div
      input(type='button' value='글쓰기' onclick='writeBoard()')
